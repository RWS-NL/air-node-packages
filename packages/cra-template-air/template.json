{
  "package": {
    "author": "@rws-air",
    "version": "1.0.0",
    "private": true,
    "proxy": "http://localhost:9009",
    "scripts": {
      "postbuild": "yarn copy:nginx",
      "test:actions": "yarn test --testPathIgnorePatterns=src/__tests__/stubrunner.test.ts",
      "lint": "run-p lint:**",
      "lint:js": "eslint src scripts --fix --ext ts,tsx",
      "lint:scss": "stylelint --fix ./src/styles/**/*.scss ./src/styles/*.scss",
      "style": "prettier --config .prettierrc --write src/**/*.{scss,tsx,ts,json}",
      "copy:nginx": "copyfiles -u 2 src/config/nginx.conf src/config/mime.types build/",
      "stub:start": "docker-compose up -d",
      "stub:stop": "docker-compose down",
      "up": "yarn upgrade-interactive --latest",
      "commit": "git-cz",
      "cz": "git-cz",
      "scripts:runner": "ts-node -P tsconfig.eslint.json",
      "scripts:validateNoOnly": "yarn scripts:runner scripts/validateNoOnly",
      "scripts:yarnAfterPull": "yarn scripts:runner scripts/yarnAfterPull"
    },
    "dependencies": {
      "@date-io/core": "^1",
      "@date-io/moment": "^1",
      "@favware/querystring": "^4.2.0",
      "@material-ui/core": "^4.9.1",
      "@material-ui/icons": "^4.9.1",
      "@material-ui/pickers": "^3.2.9",
      "@rws-air/utils": "^3.1.2",
      "@rws-air/webcomponents": "^10.1.0",
      "classnames": "^2.2.6",
      "connected-react-router": "^6.6.1",
      "core-js": "^3.6.4",
      "formik-material-ui-pickers": "^0.0.4",
      "formik-material-ui": "2.0.0-alpha.3",
      "formik": "^2.1.3",
      "fuse.js": "^3.4.6",
      "history": "^4.10.1",
      "http-status": "^1.4.2",
      "i18next-browser-languagedetector": "^4.0.1",
      "i18next": "^19.0.3",
      "lodash.debounce": "^4.0.8",
      "lodash.orderby": "^4.6.0",
      "moment": "^2.24.0",
      "react-dom": "^16.12.0",
      "react-i18next": "^11.3.1",
      "react-if": "^3.4.3",
      "react-redux-toastr": "^7.6.4",
      "react-redux": "^7.1.3",
      "react-router-dom": "^5.1.2",
      "react-router": "^5.1.2",
      "react": "^16.12.0",
      "redux-devtools-extension": "^2.13.8",
      "redux-saga": "^1.1.3",
      "redux": "^4.0.5",
      "typesafe-actions": "^5.1.0",
      "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
      "@commitlint/cli": "^8.3.5",
      "@commitlint/config-conventional": "^8.3.4",
      "@favware/ts-config": "^1.1.2",
      "@rws-air/eslint-config": "^3.0.2",
      "@rws-air/stylelint-config": "^5.0.0",
      "@types/classnames": "^2.2.9",
      "@types/enzyme": "^3.10.4",
      "@types/enzyme-adapter-react-16": "^1.0.5",
      "@types/glob": "^7.1.1",
      "@types/history": "^4.7.5",
      "@types/jest": "^25.1.1",
      "@types/lodash.debounce": "^4.0.6",
      "@types/lodash.orderby": "^4.6.6",
      "@types/node": "^13.7.0",
      "@types/react": "^16.9.19",
      "@types/react-dom": "^16.9.5",
      "@types/react-redux": "^7.1.7",
      "@types/react-redux-toastr": "^7.6.0",
      "@types/react-router": "^5.1.4",
      "@types/react-router-dom": "^5.1.3",
      "@types/redux-logger": "^3.0.7",
      "@types/redux-mock-store": "^1.0.2",
      "@typescript-eslint/eslint-plugin": "^2.18.0",
      "@typescript-eslint/parser": "^2.18.0",
      "chalk": "^3.0.0",
      "copyfiles": "^2.2.0",
      "cz-conventional-changelog": "^3.1.0",
      "enzyme": "^3.11.0",
      "enzyme-adapter-react-16": "^1.15.2",
      "enzyme-to-json": "^3.4.4",
      "eslint": "^6.8.0",
      "execa": "^4.0.0",
      "glob": "^7.1.6",
      "husky": "^4.2.1",
      "jest-enzyme": "^7.1.2",
      "jest-extended": "^0.11.4",
      "lint-staged": "^10.0.7",
      "node-sass": "^4.13.1",
      "npm-run-all": "^4.1.5",
      "prettier": "^1.19.1",
      "pretty-quick": "^2.0.1",
      "react-scripts": "^3.3.1",
      "redux-logger": "^3.0.6",
      "redux-mock-store": "^1.5.4",
      "redux-saga-test-plan": "^4.0.0-rc.3",
      "stylelint": "^13.0.0",
      "ts-node": "^8.6.2",
      "typescript": "^3.7.5",
      "utility-types": "^3.10.0"
    },
    "husky": {
      "hooks": {
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
        "pre-commit": "yarn scripts:validateNoOnly && yarn pretty-quick --staged && yarn lint-staged",
        "post-merge": "yarn scripts:yarnAfterPull"
      }
    },
    "commitlint": {
      "extends": ["@rws-air"]
    },
    "lint-staged": {
      "*.{ts,tsx}": ["eslint --fix --ext ts,tsx"],
      "*.{scss,css}": ["stylelint --fix"]
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/@rws-air/commitizen-config"
      }
    },
    "jest": {
      "coveragePathIgnorePatterns": ["<rootDir>/src/index.tsx", "<rootDir>/src/store/root-action.ts"]
    },
    "resolutions": {
      "serialize-javascript": "^3.0.0",
      "acorn": "^6.4.1",
      "minimist": "^1.2.2"
    }
  }
}
