{
  "name": "@rws-air/eslint-config-node",
  "version": "0.0.1",
  "description": "Standard ESLint config for RWS AIR projects written in TypeScript (without React)",
  "author": "@rws-air",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/RWS-NL/air-node-packages/tree/master/packages/eslint-config-node#readme",
  "scripts": {
    "test": "jest",
    "lint": "eslint src __tests__ --ext js,ts --fix -c ../../.eslintrc",
    "prebuild": "yarn lint && yarn test",
    "build": "tsc -b src",
    "start": "yarn build -w"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0"
  },
  "files": [
    "dist",
    "!dist/.tsbuildinfo"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RWS-NL/air-node-packages.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/RWS-NL/air-node-packages/issues"
  },
  "keywords": [
    "eslint-config",
    "math",
    "random",
    "security",
    "RWS-NL",
    "typescript",
    "ts"
  ],
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public"
  }
}
