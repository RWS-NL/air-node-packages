{
  "name": "@rws-air/eslint-config",
  "version": "2.1.4",
  "description": "Standard ESLint config for RWS-NL ReactJS projects written in TypeScript",
  "author": "@rws-air",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "homepage": "https://github.com/RWS-NL/air-node-packages/tree/master/packages/eslint-config#readme",
  "scripts": {
    "lint": "eslint src --fix --ext .ts,.tsx",
    "test": "yarn lint",
    "prebuild": "yarn lint",
    "build": "rollup -c rollup.config.js",
    "start": "yarn build -w"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "eslint": ">=6",
    "eslint-config-react-app": "^5",
    "eslint-plugin-flowtype": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^1"
  },
  "peerDependenciesMeta": {
    "eslint-config-react-app": {
      "optional": true
    },
    "eslint-plugin-flowtype": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "~2.6.0",
    "@typescript-eslint/parser": "~2.6.0",
    "eslint-plugin-jest": "^23.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RWS-NL/air-node-packages.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/RWS-NL/air-node-packages/issues"
  },
  "keywords": [
    "eslint",
    "config",
    "rws-air",
    "rws-nl",
    "rws",
    "air",
    "typescript",
    "ts"
  ]
}
