{
  "name": "@rws-air/stylelint-config",
  "version": "2.0.1",
  "description": "Standard StyleLint config for all AIR apps",
  "main": "dist/index.js",
  "author": "@rws-air",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "stylelint",
    "config",
    "rws",
    "air",
    "scss"
  ],
  "scripts": {
    "lint": "eslint src --fix --ext .ts,.tsx",
    "test": "yarn lint",
    "prebuild": "yarn lint",
    "build": "rollup -c rollup.config.js",
    "start": "yarn build -w",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RWS-NL/air-node-packages.git",
    "directory": "stylelint-config"
  },
  "bugs": {
    "url": "https://github.com/RWS-NL/air-node-packages/issues"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "peerDependencies": {
    "stylelint": ">=10"
  },
  "dependencies": {
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.2"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {}
}
