{
  "name": "@rws-air/stylelint-config",
  "version": "7.0.9",
  "description": "Standard StyleLint config for all AIR apps",
  "author": "@rws-air",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "homepage": "https://github.com/RWS-NL/air-node-packages/tree/master/packages/stylelint-config#readme",
  "scripts": {
    "lint": "eslint src --fix --ext .ts,.tsx",
    "test": "yarn lint",
    "prebuild": "yarn lint",
    "build": "tsc",
    "start": "tsc -w"
  },
  "files": [
    "dist",
    "!dist/.tsbuildinfo"
  ],
  "peerDependencies": {
    "stylelint": ">=14"
  },
  "dependencies": {
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-scss": "^4.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RWS-NL/air-node-packages.git",
    "directory": "packages/stylelint-config"
  },
  "bugs": {
    "url": "https://github.com/RWS-NL/air-node-packages/issues"
  },
  "keywords": [
    "stylelint",
    "config",
    "scss",
    "rws-air",
    "rws-nl",
    "rws",
    "air",
    "typescript",
    "ts"
  ]
