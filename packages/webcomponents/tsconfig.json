{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "src",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "noEmit": true,
    "downlevelIteration": true,
    "jsx": "react",
    "incremental": true,
    "target": "es6",
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["src/__tests__", "src/setupTests.ts"]
}
