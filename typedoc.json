{
  "inputFiles": ["./packages/**/src"],
  "out": "./docs/",
  "readme": "./README.md",
  "name": "RWS-NL AIR Node Packages",
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/__tests__/**",
    "**/*.js",
    "**/*.test.ts",
    "**/rollup.config.ts",
    "packages/webcomponents/src/**/index.ts",
    "packages/webcomponents/src/Router/routeQueryParser.ts",
    "packages/utils/src/**/index.ts",
    "packages/cra-template-air/**",
    "packages/local-components-test/**"
  ],
  "pathExclude": [
    "scripts/scriptUtils",
    "scripts/validateNoOnly",
    "scripts/yarnAfterPull"
  ],
  "lernaExclude": ["cra-template-air", "@rws-air/local-components-test"],
  "mode": "modules",
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeNotExported": true,
  "tsconfig": "./tsconfig.base.json"
}
