{
  "name": "@rws-air/usercreator",
  "version": "1.0.5",
  "description": "Script to add users to usermanagement environments",
  "main": "./dist/usercreator.js",
  "bin": {
    "usercreator": "./dist/usercreator.js",
    "uc": "./dist/usercreator.js"
  },
  "author": "@rws-air",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "usermanagement",
    "rws",
    "air",
    "typescript",
    "ts"
  ],
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "tslint --fix -p . -c ./tslint.json --fix",
    "start": "ts-node src/creator.ts",
    "prebuild": "yarn test && yarn clean",
    "build": "tsc",
    "postbuild": "yarn terser",
    "clean": "rimraf dist/",
    "terser": "terser --compress --mangle --output dist/usercreator.js -- dist/creator.js && rimraf dist/creator.js",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@rws-air/tslint-config-air": "^1.0.0",
    "@types/common-tags": "^1.8.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^11.13.8",
    "@types/node-fetch": "^2.3.3",
    "@types/yargs": "^13.0.0",
    "rimraf": "^2.6.3",
    "terser": "^3.17.0",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@favware/yamlreader": "^1.0.2",
    "chalk": "^2.4.2",
    "common-tags": "^1.8.0",
    "node-fetch": "^2.5.0",
    "yargs": "^13.2.2"
  },
  "git": {
    "pre-commit": "lint"
  }
}
