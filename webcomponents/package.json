{
  "name": "@rws-air/webcomponents",
  "version": "2.3.0",
  "description": "Standard Webcomponents for AIR projects",
  "author": "@rws-air",
  "license": "AGPL-3.0-or-later",
  "repository": "RWS-NL/air-node-packages",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "lint": "concurrently \"yarn tslint\" \"yarn stylelint\"",
    "tslint": "tslint --fix -p . -c ./tslint.json --fix",
    "stylelint": "stylelint --fix ./src/styles/**/*.scss ./src/styles/*.scss ./src/**/*.scss",
    "prebuild": "yarn clean",
    "build": "rollup -c rollup.config.ts",
    "postbuild": "yarn terser",
    "start": "yarn build -w",
    "start:local": "pushd local-sample && yarn install && yarn start",
    "clean": "rimraf ./dist",
    "terser": "terser-folder ./dist -eo ./dist -x .js",
    "prepublishOnly": "yarn lint && yarn test:ci && git co tsconfig.json && yarn build",
    "modules": "rimraf node_modules yarn.lock local-sample/node_modules local-sample/yarn.lock && yarn && pushd local-sample && yarn && popd"
  },
  "dependencies": {},
  "peerDependencies": {
    "@material-ui/core": ">=4",
    "@material-ui/icons": ">=4",
    "classnames": ">=2",
    "lodash.debounce": ">=4",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@rws-air/stylelint-config": "^1.0.0",
    "@rws-air/tslint-config": "^1.0.1",
    "@svgr/rollup": "^4.3.1",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.7",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-typescript2": "^0.21.2",
    "rollup-plugin-url": "^2.2.2",
    "stylelint": "^10.1.0",
    "terser-folder": "^2.0.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "utility-types": "^3.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist"
  ]
}
